   "source": [
    "S = list(input()) # 입력 값 리스트로 받기\n",
    "\n",
    "nums = [int(i) for i in S]\n",
    "result = 0\n",
    "\n",
    "for num in nums:\n",
    "    # num이나 result 값이 0, 1의 값을 가질 때, 덧셈 진행\n",
    "    if num in [0, 1] or result in [0, 1]: \n",
    "        result += num\n",
    "    else:\n",
    "    # 그 외 곱셈 진행\n",
    "        result *= num\n",
    "        \n",
    "print(result)"
   ]
